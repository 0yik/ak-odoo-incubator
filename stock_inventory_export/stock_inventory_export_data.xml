<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="stock_inventory_export_profile" model="ir.exports">
            <field name="name">Inventory Adjustment</field>
            <field name="resource">stock.inventory</field>
        </record>

        <record id="stock_inventory_export_product_name" model="ir.exports.line">
            <field name="name">line_ids/product_id/id</field>
            <field name="export_id" ref="stock_inventory_export_valuation"/>
        </record>

        <record id="stock_inventory_export_product_uom" model="ir.exports.line">
            <field name="name">line_ids/product_uom_id/id</field>
            <field name="export_id" ref="stock_inventory_export_valuation"/>
        </record>

        <record id="stock_inventory_export_location_line" model="ir.exports.line">
            <field name="name">line_ids/location_id/id</field>
            <field name="export_id" ref="stock_inventory_export_valuation"/>
        </record>

        <record id="stock_inventory_export_product_qty" model="ir.exports.line">
            <field name="name">line_ids/product_qty</field>
            <field name="export_id" ref="stock_inventory_export_valuation"/>
        </record>

    </data>
</openerp>
