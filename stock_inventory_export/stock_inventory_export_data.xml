        <record id="stock_inventory_export_location_name" model="ir.exports.line">
            <field name="name">line_ids/location_name</field>
            <field name="export_id" ref="stock_inventory_export_profile"/>
        </record>

        <record id="stock_inventory_export_prod_lot_id" model="ir.exports.line">
            <field name="name">line_ids/prod_lot_id</field>
            <field name="export_id" ref="stock_inventory_export_profile"/>
