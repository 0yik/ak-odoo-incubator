<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="stock_inventory_export_profile" model="ir.exports">
            <field name="name">Inventory Adjustment</field>
            <field name="resource">stock.inventory</field>
        </record>

        <record id="stock_inventory_export_product_id" model="ir.exports.line">
            <field name="name">line_ids/product_id/id</field>
