<odoo>

    <record id="training_odoo_12_quote" model="sale.order">
        <field name="partner_id" ref="res_partner_dih"/>
        <field name="partner_invoice_id" ref="res_partner_dih"/>
        <field name="partner_shipping_id" ref="res_partner_dih"/>
        <field name="date_order" eval="(DateTime.today() - relativedelta(days=1)).strftime('%Y-%m-%d %H:%M')"/>
    </record>
    <record id="training_odoo_12_quote_line_1" model="sale.order.line">
        <field name="order_id" ref="training_odoo_12_quote"/>
